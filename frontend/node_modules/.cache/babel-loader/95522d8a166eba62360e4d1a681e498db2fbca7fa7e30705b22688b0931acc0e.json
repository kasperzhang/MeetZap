 }\n  },\n\n  created() {\n    let { email } = this.$route.query\n    post(`/events/${this.eventId}/responded`, { email })\n      .then(() => {\n        this.state = this.states.CONFIRMED\n      })\n      .catch((err) => {\n        console.error(err)\n        this.state = this.states.ERROR\n      })\n  },\n}\n</script>\n"],"mappings":"AAwBA,SAAAA,IAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACAC,OAAA;MAAAC,IAAA,EAAAC,MAAA;MAAAC,QAAA;IAAA;EACA;EAEAC,KAAA;IACA;MACAC,KAAA;MACAC,MAAA;QACAC,UAAA;QACAC,SAAA;QACAC,KAAA;MACA;IACA;EACA;EAEAC,QAAA;IACA;MAAAC;IAAA,SAAAC,MAAA,CAAAC,KAAA;IACAhB,IAAA,iBAAAG,OAAA;MAAAW;IAAA,GACAG,IAAA;MACA,KAAAT,KAAA,QAAAC,MAAA,CAAAE,SAAA;IACA,GACAO,KAAA,CAAAC,GAAA;MACAC,OAAA,CAAAC,KAAA,CAAAF,GAAA;MACA,KAAAX,KAAA,QAAAC,MAAA,CAAAG,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}